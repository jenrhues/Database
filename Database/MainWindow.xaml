<Window x:Class="Database.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Database"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="typeHere" HorizontalAlignment="Left" Height="23" Margin="221,251,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" TextChanged="typeHere_TextChanged"/>
        <Button x:Name="assets" Content="See Assets" HorizontalAlignment="Left" Margin="221,213,0,0" VerticalAlignment="Top" Width="75" Click="assets_Click"/>
        <TextBlock x:Name="Display" HorizontalAlignment="Left" Margin="371,200,0,0" TextWrapping="Wrap" Text="Peanut Butter Jelly TIME" VerticalAlignment="Top" Height="66" Width="43" TextAlignment="Center"/>

    </Grid>
</Window>
